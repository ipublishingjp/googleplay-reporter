{
    "name": "ipublishingjp/googleplay-reporter",
    "description": "PHP package for downloading stats from GooglePlay",
    "keywords": ["Google", "Play"],
    "homepage": "https://github.com/ipublishingjp/googleplay-reporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Yukimune Takagi",
            "email": "takagi@ipublishing.jp",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "name": "snscripts/result",
            "type": "vcs",
            "url": "https://github.com/ipublishingjp/result"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-mbstring": "*",
        "google/cloud-storage": "^1.5",
        "snscripts/result": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "IPublishingjp\\GooglePlayReporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPublishingjp\\GooglePlayReporter\\Tests\\": "tests"
        }
    }
}
